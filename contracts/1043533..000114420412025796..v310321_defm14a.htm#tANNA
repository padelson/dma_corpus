<TR STYLE="background-color: white">

          <TD STYLE="text-align: center; vertical-align: text-bottom" ROWSPAN=1 COLSPAN=1>Financial Party B<BR></TD>

          <TD STYLE="width: 6px; vertical-align: text-bottom; text-align: center" ROWSPAN=1><!-- GUTTER -->&nbsp;</TD>

          <TD STYLE="width: 6px; vertical-align: text-bottom; text-align: left" ROWSPAN=1><!-- $ -->&nbsp;</TD>

          <TD STYLE="vertical-align: text-bottom; text-align: center" ROWSPAN=1 COLSPAN=1>&#36;75.00&nbsp;&#150;&nbsp;&#36;80.00</TD>

          <TD STYLE="white-space: nowrap; vertical-align: text-bottom; text-align: left"><!-- PERCENT -->&nbsp;</TD>

</TR>

<TR STYLE="background-color: #CCFFCC">

          <TD STYLE="text-align: center; vertical-align: text-bottom" ROWSPAN=1 COLSPAN=1>Financial Party C<BR></TD>

          <TD STYLE="width: 6px; vertical-align: text-bottom; text-align: center" ROWSPAN=1><!-- GUTTER -->&nbsp;</TD>

          <TD STYLE="width: 6px; vertical-align: text-bottom; text-align: left" ROWSPAN=1><!-- $ -->&nbsp;</TD>

          <TD STYLE="vertical-align: text-bottom; text-align: center" ROWSPAN=1 COLSPAN=1>&#36;75.00&nbsp;&#150;&nbsp;&#36;79.00</TD>

          <TD STYLE="white-space: nowrap; vertical-align: text-bottom; text-align: left"><!-- PERCENT -->&nbsp;</TD>

</TR>

<TR STYLE="background-color: white">

          <TD STYLE="text-align: center; vertical-align: text-bottom" ROWSPAN=1 COLSPAN=1>Financial Party D<BR></TD>

          <TD STYLE="width: 6px; vertical-align: text-bottom; text-align: center" ROWSPAN=1><!-- GUTTER -->&nbsp;</TD>

          <TD STYLE="width: 6px; vertical-align: text-bottom; text-align: left" ROWSPAN=1><!-- $ -->&nbsp;</TD>

          <TD STYLE="vertical-align: text-bottom; text-align: center" ROWSPAN=1 COLSPAN=1>&#36;72.00&nbsp;&#150;&nbsp;&#36;74.00</TD>

          <TD STYLE="white-space: nowrap; vertical-align: text-bottom; text-align: left"><!-- PERCENT -->&nbsp;</TD>

