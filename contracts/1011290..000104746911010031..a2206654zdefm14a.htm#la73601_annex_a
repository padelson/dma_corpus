<!-- ZEQ.=8,SEQ=90,EFW="2206654",CP="99 CENTS ONLY STORES",DN="1",CHK=382757,FOLIO='87',FILE='DISK126:[11ZDN1.11ZDN73601]CW73601A.;49',USER='GKIBBE',CD=';9-DEC-2011;15:24' -->
