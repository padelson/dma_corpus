<!-- ZEQ.=1,SEQ=58,EFW="2206654",CP="99 CENTS ONLY STORES",DN="1",CHK=641843,FOLIO='55',FILE='DISK126:[11ZDN1.11ZDN73601]CM73601A.;58',USER='GKIBBE',CD=';9-DEC-2011;15:24' -->
